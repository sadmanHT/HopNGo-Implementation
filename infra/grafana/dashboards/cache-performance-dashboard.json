{
  "dashboard": {
    "id": null,
    "title": "HopNGo Cache Performance Dashboard",
    "tags": ["hopngo", "cache", "performance", "redis", "nginx"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "query": "label_values(cache_hit_ratio, service)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*",
          "current": {
            "text": "All",
            "value": "$__all"
          }
        },
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(cache_hit_ratio{service=~\"$service\"}, instance)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*",
          "current": {
            "text": "All",
            "value": "$__all"
          }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Cache Hit Ratio Overview",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "avg(cache_hit_ratio{service=~\"$service\", instance=~\"$instance\"}) * 100",
            "legendFormat": "Overall Hit Ratio",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 85}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 2,
        "title": "Cache Miss Ratio",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "avg(cache_miss_ratio{service=~\"$service\", instance=~\"$instance\"}) * 100",
            "legendFormat": "Overall Miss Ratio",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 15},
                {"color": "red", "value": 30}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 3,
        "title": "Cache Memory Usage",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "avg(redis_memory_used_bytes{service=~\"$service\", instance=~\"$instance\"}) / avg(redis_memory_max_bytes{service=~\"$service\", instance=~\"$instance\"}) * 100",
            "legendFormat": "Memory Usage",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 85}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 4,
        "title": "Cache Operations/sec",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(cache_operations_total{service=~\"$service\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "Operations/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "decimals": 1,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 5000}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 5,
        "title": "Cache Hit Ratio by Service",
        "type": "timeseries",
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "cache_hit_ratio{service=~\"$service\", instance=~\"$instance\"} * 100",
            "legendFormat": "{{service}} - {{instance}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"}
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 70}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        }
      },
      {
        "id": 6,
        "title": "Cache Response Time",
        "type": "timeseries",
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(cache_request_duration_seconds_bucket{service=~\"$service\", instance=~\"$instance\"}[5m])) * 1000",
            "legendFormat": "95th percentile - {{service}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(cache_request_duration_seconds_bucket{service=~\"$service\", instance=~\"$instance\"}[5m])) * 1000",
            "legendFormat": "50th percentile - {{service}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"}
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 200}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        }
      },
      {
        "id": 7,
        "title": "Redis Memory Usage Details",
        "type": "timeseries",
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 17},
        "targets": [
          {
            "expr": "redis_memory_used_bytes{service=~\"$service\", instance=~\"$instance\"}",
            "legendFormat": "Used Memory - {{instance}}",
            "refId": "A"
          },
          {
            "expr": "redis_memory_max_bytes{service=~\"$service\", instance=~\"$instance\"}",
            "legendFormat": "Max Memory - {{instance}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"}
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        }
      },
      {
        "id": 8,
        "title": "Cache Evictions",
        "type": "timeseries",
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 17},
        "targets": [
          {
            "expr": "rate(redis_evicted_keys_total{service=~\"$service\", instance=~\"$instance\"}[5m])",
            "legendFormat": "Evictions/sec - {{instance}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"}
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        }
      },
      {
        "id": 9,
        "title": "Nginx Cache Status",
        "type": "piechart",
        "gridPos": {"h": 9, "w": 8, "x": 0, "y": 26},
        "targets": [
          {
            "expr": "sum by (status) (rate(nginx_cache_requests_total{service=\"frontend\"}[5m]))",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [
              {
                "options": {
                  "HIT": {"color": "green", "index": 0},
                  "MISS": {"color": "red", "index": 1},
                  "BYPASS": {"color": "yellow", "index": 2},
                  "EXPIRED": {"color": "orange", "index": 3}
                },
                "type": "value"
              }
            ]
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "visible", "placement": "right"},
          "displayLabels": ["name", "percent"]
        }
      },
      {
        "id": 10,
        "title": "Cache Key Distribution",
        "type": "bargauge",
        "gridPos": {"h": 9, "w": 8, "x": 8, "y": 26},
        "targets": [
          {
            "expr": "topk(10, redis_db_keys{service=~\"$service\", instance=~\"$instance\"})",
            "legendFormat": "DB {{db}} - {{instance}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "orientation": "horizontal",
              "displayMode": "gradient"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10000},
                {"color": "red", "value": 50000}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "id": 11,
        "title": "Cache Connection Pool",
        "type": "timeseries",
        "gridPos": {"h": 9, "w": 8, "x": 16, "y": 26},
        "targets": [
          {
            "expr": "redis_connected_clients{service=~\"$service\", instance=~\"$instance\"}",
            "legendFormat": "Connected Clients - {{instance}}",
            "refId": "A"
          },
          {
            "expr": "redis_blocked_clients{service=~\"$service\", instance=~\"$instance\"}",
            "legendFormat": "Blocked Clients - {{instance}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"}
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 100}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Cache Alerts",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertname=~\".*Cache.*\", service=~\"$service\"}",
          "iconColor": "red",
          "titleFormat": "{{alertname}}",
          "textFormat": "{{instance}}: {{summary}}"
        }
      ]
    },
    "links": [
      {
        "title": "Cache Alerts",
        "url": "/d/alerts/alerts?var-service=$service",
        "type": "dashboards"
      },
      {
        "title": "Application Performance",
        "url": "/d/app-performance/application-performance?var-service=$service",
        "type": "dashboards"
      }
    ]
  }
}