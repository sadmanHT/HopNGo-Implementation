<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- 
        This file contains suppressions for known false positives in OWASP Dependency Check.
        Each suppression should include:
        1. A clear reason for suppression
        2. The CVE or vulnerability being suppressed
        3. The affected file/dependency
        4. An expiration date for review
    -->
    
    <!-- Example suppressions - customize based on your actual dependencies -->
    
    <!-- Spring Boot Test Dependencies - Development Only -->
    <suppress>
        <notes><![CDATA[
            Spring Boot test dependencies are only used during development and testing.
            These vulnerabilities do not affect production deployments.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring\-boot\-starter\-test@.*$</packageUrl>
        <cve>CVE-2023-20861</cve>
        <cve>CVE-2023-20863</cve>
    </suppress>
    
    <!-- H2 Database - Development/Test Only -->
    <suppress>
        <notes><![CDATA[
            H2 database is only used for development and testing environments.
            Production uses PostgreSQL which is not affected by H2 vulnerabilities.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.h2database/h2@.*$</packageUrl>
        <cve>CVE-2022-45868</cve>
    </suppress>
    
    <!-- Tomcat Embed - Managed by Spring Boot -->
    <suppress>
        <notes><![CDATA[
            Tomcat embedded vulnerabilities are managed by Spring Boot updates.
            We regularly update Spring Boot to get the latest Tomcat security fixes.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.tomcat\.embed/.*@.*$</packageUrl>
        <vulnerabilityName>CVE-2023-28708</vulnerabilityName>
    </suppress>
    
    <!-- Jackson Databind - False Positive for Specific Usage -->
    <suppress>
        <notes><![CDATA[
            This Jackson vulnerability requires specific conditions that don't apply to our usage.
            We don't deserialize untrusted data with polymorphic types.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.fasterxml\.jackson\.core/jackson\-databind@.*$</packageUrl>
        <cve>CVE-2023-35116</cve>
    </suppress>
    
    <!-- Netty - Specific Version Range -->
    <suppress until="2024-12-31">
        <notes><![CDATA[
            Netty vulnerability affects specific usage patterns not present in our application.
            Scheduled for review and update by end of 2024.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/io\.netty/netty\-.*@4\.1\.[0-9]+.*$</packageUrl>
        <cve>CVE-2023-44487</cve>
    </suppress>
    
    <!-- Logback - Configuration Controlled -->
    <suppress>
        <notes><![CDATA[
            Logback vulnerability requires specific configuration that we don't use.
            Our logging configuration is secure and doesn't enable vulnerable features.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/ch\.qos\.logback/logback\-.*@.*$</packageUrl>
        <cve>CVE-2023-6378</cve>
    </suppress>
    
    <!-- SnakeYAML - Safe Usage -->
    <suppress until="2024-06-30">
        <notes><![CDATA[
            SnakeYAML vulnerabilities require deserializing untrusted YAML.
            We only parse trusted configuration files. Scheduled for library update.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.yaml/snakeyaml@.*$</packageUrl>
        <cve>CVE-2022-1471</cve>
        <cve>CVE-2022-38749</cve>
        <cve>CVE-2022-38750</cve>
    </suppress>
    
    <!-- Node.js Development Dependencies -->
    <suppress>
        <notes><![CDATA[
            Development dependencies are not included in production builds.
            These vulnerabilities only affect the development environment.
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/.*@.*$</packageUrl>
        <cve>CVE-2023-26136</cve>
        <cve>CVE-2023-26115</cve>
    </suppress>
    
    <!-- Webpack Dev Server - Development Only -->
    <suppress>
        <notes><![CDATA[
            Webpack dev server is only used during development.
            Production builds use static files served by nginx.
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/webpack\-dev\-server@.*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>
    
    <!-- Jest Testing Framework - Development Only -->
    <suppress>
        <notes><![CDATA[
            Jest is only used for running tests during development and CI.
            Not included in production deployments.
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/jest@.*$</packageUrl>
        <packageUrl regex="true">^pkg:npm/@jest/.*@.*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>
    
    <!-- ESLint and related tools - Development Only -->
    <suppress>
        <notes><![CDATA[
            ESLint and related linting tools are only used during development.
            Not included in production builds.
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/eslint.*@.*$</packageUrl>
        <packageUrl regex="true">^pkg:npm/@typescript\-eslint/.*@.*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>
    
    <!-- Temporary suppressions for known issues under investigation -->
    
    <!-- Redis Client - Specific Version -->
    <suppress until="2024-03-31">
        <notes><![CDATA[
            Redis client vulnerability under investigation.
            Temporary suppression while evaluating impact and upgrade path.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/redis\.clients/jedis@.*$</packageUrl>
        <cve>CVE-2023-41053</cve>
    </suppress>
    
    <!-- PostgreSQL Driver - Version Specific -->
    <suppress until="2024-04-30">
        <notes><![CDATA[
            PostgreSQL driver vulnerability requires specific conditions.
            Under review for upgrade compatibility with current database version.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.postgresql/postgresql@.*$</packageUrl>
        <cve>CVE-2024-1597</cve>
    </suppress>
    
</suppressions>